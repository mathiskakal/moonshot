{
  "name": "rexmax",
  "author": "Mathis KAKAL (thiska)",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",  "scripts": {
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "npm run transpile:electron && cross-env NODE_ENV=development electron .",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "transpile:electron": "tsc --project src/electron/tsconfig.json",
    "rebuild:better-sqlite3": "electron-rebuild -f -w better-sqlite3",
    "postinstall": "electron-rebuild -f -w better-sqlite3",
    "dist:mac": "npm run transpile:electron && npm run build && npm run rebuild:better-sqlite3 && electron-builder --mac --arm64",
    "dist:win": "npm run transpile:electron && npm run build && npm run rebuild:better-sqlite3 && electron-builder --win --x64",
    "dist:linux": "npm run transpile:electron && npm run build && npm run rebuild:better-sqlite3 && electron-builder --linux --x64"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "better-sqlite3": "^11.10.0",
    "dotenv": "^16.5.0",
    "odbc": "^2.4.9",
    "os-utils": "^0.0.14",
    "pino": "^9.7.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/electron": "^1.4.38",
    "@types/os-utils": "^0.0.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "cross-env": "^7.0.3",
    "electron": "^36.1.0",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "memfs": "^4.17.2",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^13.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  }
}
